{
  "name": "usermanagementapp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "express": "^4",
    "hdb": "^0.18.3"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.96.5",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run",
    "undeploy": "cf undeploy usermanagementapp --delete-services --delete-service-keys"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "sapux": [
    "app/usermanagementapp_fiori"
  ],
  "cds": {
    "requires": {
      "ZC_USERS_CDS": {
        "kind": "odata-v2",
        "model": "srv/external/ZC_USERS_CDS",
        "credentials": {
          "destination": "PSD_HTTPS_TEST",
          "path": "/sap/opu/odata/sap/ZC_USERS_CDS"
        }
      },
      "db": {
        "kind": "hana"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}
